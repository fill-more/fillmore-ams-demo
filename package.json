{
  "name": "fillmore-monorepo",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev:educator": "pnpm --filter fillmore-educator dev",
    "dev:educator:vercel": "vercel dev -A ./apps/fillmore-educator/vercel.local.json",
    "dev:learner": "pnpm --filter fillmore-learner dev",
    "build": "pnpm --filter \"./apps/**\" build",
    "lint": "pnpm --filter \"./**\" lint",
    "prepare": "husky",
    "generate:supabase-types": "npx supabase gen types typescript --project-id zyaottgyyvvxhklkbckq > packages/db/types/supabase.ts"
  },
  "devDependencies": {
    "@eslint/js": "^9.33.0",
    "eslint": "^9.33.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-prettier": "^5.5.4",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.20",
    "globals": "^16.3.0",
    "husky": "^9.1.7",
    "lint-staged": "^16.1.6",
    "prettier": "^3.6.2",
    "typescript": "~5.8.3",
    "typescript-eslint": "^8.39.1"
  },
  "lint-staged": {
    "*.{ts,tsx,js,jsx}": [
      "eslint --fix",
      "prettier --write"
    ]
  },
  "dependencies": {
    "@supabase/supabase-js": "^2.57.4"
  }
}
